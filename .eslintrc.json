{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-console": "off",
		"linebreak-style": "off"
	},
	"import/no-extraneous-dependencies": [
		"error",
		{
			"devDependencies": false,
			"optionalDependencies": false,
			"peerDependencies": false
		}
	]
}
